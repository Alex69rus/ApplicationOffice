            "name": "Approvals.Api",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build:ApplicationOffice.Approvals",
            "program": "${workspaceFolder}/ApplicationOffice.Approvals/ApplicationOffice.Approvals.Api/bin/Debug/net5.0/ApplicationOffice.Approvals.Api.dll",
